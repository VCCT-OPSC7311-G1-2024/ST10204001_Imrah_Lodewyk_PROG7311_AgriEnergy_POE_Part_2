<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>Agri-Energy</title>
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
	<!-- Syncfusion ASP.NET Core controls styles -->
	@* <link rel="stylesheet" href="https://cdn.syncfusion.com/ej2/25.2.3/material.css" /> *@
	<link href="https://cdn.syncfusion.com/ej2/25.2.3/bootstrap5-dark.css" rel="stylesheet" />
	<!-- Font awesome -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
	<!-- Inter font -->
	<link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;700&family=Roboto:wght@300;400;700&display=swap" rel="stylesheet">
	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/AgriEnergy_ST10204001_POE_Part_2.styles.css" asp-append-version="true" />
	<!-- Roboto font -->
	<link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;700&display=swap" rel="stylesheet">
	<!-- Syncfusion ASP.NET Core controls styles -->
	<script src="https://cdn.syncfusion.com/ej2/25.2.3/dist/ej2.min.js"></script>
	<link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
</head>

<body class="e-bigger">

	<div>
		<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom box-shadow mb-3">
			<div class="container-fluid">
				<a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index" style="color: white;font-family: Ink Free;font-size: 30px;">Agri-Energy</a>
				<div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
					<ul class="navbar-nav flex-grow-1">
					</ul>
					<div class="search-container">
						<form>
							<input type="text" placeholder="Search.." name="search">
							<button type="submit"><i class="fa fa-search"></i></button>
						</form>
					</div>
					<partial name="_LoginPartial" />
				</div>
			</div>
		</nav>
	</div>

	<div class="container">
		<main role="main" class="pb-3">
			@RenderBody()
		</main>
	</div>

	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	@await RenderSectionAsync("Scripts", required: false)

	<!-- Syncfusion ASP.NET Core Script Manager -->
	<ejs-scripts></ejs-scripts>
</body>
</html>
